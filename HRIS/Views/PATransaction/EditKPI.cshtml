@model APPBASE.Models.PATransaction_DetailVM
@using APPBASE.Helpers

@{
    ViewBag.Title = "Penilaian KPI";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section includeBOTTOM {
    @Scripts.Render("~/bundles/JSInputMask")
}

@using (Html.BeginForm())
{

@Html.HiddenFor(model => model.RUID)
<div class="row">
    <div class="col-xs-12">
        <div class="box box-info">
            <div class="box-header">
                <h3 class="box-title"></h3>
            </div><!-- /.box-header -->
            <div class="box-body">
                @{Html.RenderPartial("_Inputs_KPI");}
            </div><!-- /.box-body -->
            <div class="box-footer">
                <div class="row">
                    <div class="col-xs-12">
                        <button type="submit" class="btn btn-primary btn-flat">Simpan Data</button>
                        @Html.ActionLink("Back to List", "Index")
                    </div>
                </div><!-- /.row -->
            </div><!-- /.box-footer -->
        </div><!-- /.box -->
    </div> <!-- /.col-xs-12 -->
</div> <!-- /.row -->

}

<!-- Modal -->
@{Html.RenderPartial("_PartialDialogLookup");}


@section scripts {

<!-- Modal -->
@{Html.RenderPartial("_Events");}

}

